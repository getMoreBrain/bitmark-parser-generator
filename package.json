{
  "name": "@gmb/bitmark-parser-generator",
  "version": "3.25.1",
  "description": "A bitmark parser and generator using Peggy.js",
  "author": "Get More Brain Ltd",
  "license": "ISC",
  "homepage": "https://github.com/getMoreBrain/bitmark-parser-generator#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "run start-parser",
    "start-info": "node --import tsx ./test/custom/dev/devInfo.ts",
    "start-parser": "node --import tsx ./test/custom/dev/devParser.ts",
    "start-generator": "node --import tsx ./test/custom/dev/devGenerator.ts",
    "start-prettify": "node --import tsx ./test/custom/dev/devPrettify.ts",
    "start-text-plusplus-parser": "node --import tsx ./test/custom/dev/devTextPlusPlusParser.ts",
    "start-text-plusplus-generator": "node --import tsx ./test/custom/dev/devTextPlusPlusGenerator.ts",
    "start-text-minusminus-parser": "node --import tsx ./test/custom/dev/devTextMinusMinusParser.ts",
    "start-text-minusminus-generator": "node --import tsx ./test/custom/dev/devTextMinusMinusGenerator.ts",
    "start-text-plusplus-breakscape": "node --import tsx ./test/custom/dev/devTextPlusPlusBreakscape.ts",
    "start-text-plusplus-unbreakscape": "node --import tsx ./test/custom/dev/devTextPlusPlusUnbreakscape.ts",
    "start-text-minusminus-breakscape": "node --import tsx ./test/custom/dev/devTextMinusMinusBreakscape.ts",
    "start-text-minusminus-unbreakscape": "node --import tsx ./test/custom/dev/devTextMinusMinusUnbreakscape.ts",
    "start-text-plain-breakscape": "node --import tsx ./test/custom/dev/devTextPlainBreakscape.ts",
    "start-text-plain-unbreakscape": "node --import tsx ./test/custom/dev/devTextPlainUnbreakscape.ts",
    "start-text-tag-breakscape": "node --import tsx ./test/custom/dev/devTextTagBreakscape.ts",
    "start-text-tag-unbreakscape": "node --import tsx ./test/custom/dev/devTextTagUnbreakscape.ts",
    "build": "run clean && run generate-build-info && run build-grammar-bit && run build-grammar-text && run lint && run build-cjs && run build-esm && run build-umd && run build-supported-info",
    "build-cjs": "tsc",
    "build-esm": "tsc --module ES6 --outDir 'dist/esm'",
    "build-umd": "webpack",
    "build-doc": "typedoc",
    "build-grammar-bit": "node --import tsx ./scripts/grammar/bitmark/generate-bitmark-parser.ts --bit",
    "build-grammar-bit-test": "BPG_ENV=development node --import tsx ./scripts/grammar/bitmark/generate-bitmark-parser.ts --bit --test",
    "build-grammar-text": "node --import tsx ./scripts/grammar/text/generate-text-parser.ts",
    "build-grammar-text-test": "BPG_ENV=development node --import tsx ./scripts/grammar/text/generate-text-parser.ts --test",
    "build-supported-info": "node --import tsx ./scripts/generate-supported-bits-info.ts",
    "test": "jest --runInBand",
    "test-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-generator.test.ts --runInBand",
    "test-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-parser.test.ts --runInBand",
    "test-text-plusplus-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-plusplus-parser.test.ts --runInBand",
    "test-text-plusplus-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-plusplus-generator.test.ts --runInBand",
    "test-text-minusminus-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-minusminus-parser.test.ts --runInBand",
    "test-text-minusminus-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-minusminus-generator.test.ts --runInBand",
    "test-text-plain-breakscape": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-plain-breakscape.test.ts --runInBand",
    "test-text-plain-unbreakscape": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-plain-unbreakscape.test.ts --runInBand",
    "test-text-tag-breakscape": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-tag-breakscape.test.ts --runInBand",
    "test-text-tag-unbreakscape": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-tag-unbreakscape.test.ts --runInBand",
    "test-web-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/web-parser.test.ts --runInBand",
    "test-web-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/web-generator.test.ts --runInBand",
    "generate-build-info": "node ./scripts/generate-build-info.js",
    "publish": "yarn npm publish",
    "prepublish": "run clean && run build && run test",
    "clean": "shx rm -rf ./dist",
    "lint": "eslint --max-warnings 0 -- './**/*.@(tsx|ts|jsx|js|mjs|cjs)'",
    "upgrade": "yarn upgrade-interactive",
    "tsc": "tsc",
    "jest": "jest"
  },
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=10",
    "npm": ">=6.8.0",
    "git": ">=2.11.0",
    "yarn": ">=3.5.0"
  },
  "keywords": [
    "bitmark"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getMoreBrain/bitmark-parser-generator.git"
  },
  "browserslist": {
    "modern": [
      "last 5 chrome versions",
      "last 5 firefox versions"
    ]
  },
  "jsdelivr": "dist/browser/bitmark-parser-generator.min.js",
  "unpkg": "dist/browser/bitmark-parser-generator.min.js",
  "contributors": [
    "Richard Sewell (https://github.com/six5536)"
  ],
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@types/ungap__structured-clone": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "diff": "^8.0.1",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-arca": "^0.16.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporter": "^4.1.0",
    "jest-junit": "^16.0.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "peggy": "^4.2.0",
    "prettier": "^3.5.3",
    "shx": "^0.4.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "5.8.2",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-strip-block": "^0.3.0"
  },
  "dependencies": {
    "@ncoderz/superenum": "^0.2.5",
    "@types/fs-extra": "^11.0.4",
    "@ungap/structured-clone": "^1.3.0",
    "fs-extra": "^11.3.0"
  }
}
