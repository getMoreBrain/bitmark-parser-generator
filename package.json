{
  "name": "@getmorebrain/bitmark-parser-generator",
  "version": "1.0.3",
  "description": "A bitmark parser and generator using Peggy.js",
  "author": "Richard Sewell",
  "license": "ISC",
  "homepage": "https://github.com/getMoreBrain/bitmark-parser-generator#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "run start-parser",
    "start-parser": "node -r esbuild-register ./test/custom/dev/devParser.ts",
    "start-parser-antlr": "node -r esbuild-register ./test/custom/dev/devParserAntlr.ts",
    "start-generator": "node -r esbuild-register ./test/custom/dev/devGenerator.ts",
    "start-prettify": "node -r esbuild-register ./test/custom/dev/devPrettify.ts",
    "start-text-parser": "node -r esbuild-register ./test/custom/dev/devTextParser.ts",
    "start-text-generator": "node -r esbuild-register ./test/custom/dev/devTextGenerator.ts",
    "build": "run clean && run generate-build-info && run build-grammar-bit && run build-grammar-text && run lint && run build-cjs && run build-esm && run build-umd && run build-umd-antlr",
    "build-cjs": "tsc",
    "build-esm": "tsc --module ES6 --outDir 'dist/esm'",
    "build-umd": "webpack",
    "build-umd-antlr": "webpack --config webpack.antlr.config.ts",
    "build-doc": "typedoc",
    "build-grammar-bit": "node -r esbuild-register ./scripts/grammar/bitmark/generate-bitmark-parser.ts --bit",
    "build-grammar-bit-test": "BPG_ENV=development node -r esbuild-register ./scripts/grammar/bitmark/generate-bitmark-parser.ts --bit --test",
    "build-grammar-text": "node -r esbuild-register ./scripts/grammar/text/generate-text-parser.ts",
    "build-grammar-text-test": "BPG_ENV=development node -r esbuild-register ./scripts/grammar/text/generate-text-parser.ts --test",
    "test": "jest --runInBand",
    "test-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-generator.test.ts --runInBand",
    "test-generator-books": "jest --config=./test/custom/generator/books/jest.generator.books.config.js --runInBand",
    "test-generator-expected": "jest --config=./test/custom/generator/expected/jest.generator.expected.config.js --runInBand",
    "test-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-parser.test.ts --runInBand",
    "test-parser-books": "jest --config=./test/custom/parser/books/jest.parser.books.config.js --runInBand",
    "test-parser-expected": "jest --config=./test/custom/parser/expected/jest.parser.expected.config.js --runInBand",
    "test-text-parser": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-parser.test.ts --runInBand",
    "test-text-generator": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/text-generator.test.ts --runInBand",
    "generate-build-info": "node ./scripts/generate-build-info.js",
    "publish": "yarn npm publish",
    "prepublish": "run lint && run clean && run build && run test",
    "clean": "shx rm -rf ./dist",
    "lint": "eslint --max-warnings 0 -- './**/*.@(tsx|ts|jsx|js|mjs|cjs)'",
    "ncu": "yarn dlx npm-check-updates -i",
    "tsc": "tsc",
    "jest": "jest"
  },
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=10",
    "npm": ">=6.8.0",
    "git": ">=2.11.0",
    "yarn": ">=3.5.0"
  },
  "keywords": [
    "bitmark"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getMoreBrain/bitmark-parser-generator.git"
  },
  "browserslist": {
    "modern": [
      "last 5 chrome versions",
      "last 5 firefox versions"
    ]
  },
  "jsdelivr": "dist/browser/bitmark-parser-generator.min.js",
  "unpkg": "dist/browser/bitmark-parser-generator.min.js",
  "contributors": [
    "Richard Sewell (https://github.com/zx-ncoderz)"
  ],
  "resolutions": {
    "antlr4ts": "0.5.0-alpha.4",
    "npm-exists": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.2.1.tgz"
  },
  "overrides": {
    "antlr4ts": "0.5.0-alpha.4",
    "npm-exists": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.2.1.tgz"
  },
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "esbuild": "^0.18.4",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-arca": "^0.16.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jest-html-reporter": "^3.10.0",
    "jest-junit": "^16.0.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "peggy": "^3.0.2",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "5.0.4",
    "webpack": "^5.87.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-strip-block": "^0.3.0"
  },
  "dependencies": {
    "@ncoderz/superenum": "^0.2.0",
    "@types/fs-extra": "^11.0.1",
    "bitmark-grammar": "1.0.74",
    "fs-extra": "^11.1.1"
  }
}
