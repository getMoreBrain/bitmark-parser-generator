{
  "name": "@getmorebrain/bitmark-generator",
  "version": "0.0.1",
  "description": "A bitmark generator",
  "author": "Richard Sewell",
  "license": "ISC",
  "homepage": "https://github.com/bitmark-standard/bitmark-generator#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node -r esbuild-register ./test/custom/dev/bmgDevJson.ts",
    "start-bit": "node -r esbuild-register ./test/custom/dev/bmgDevBitmark.ts",
    "start-bit-antlr": "node -r esbuild-register ./test/custom/dev/bmgDevBitmarkAntlr.ts",
    "build": "run build-cjs && run build-esm && webpack && run build-doc",
    "build-cjs": "tsc",
    "build-esm": "tsc --module ES6 --outDir 'dist/esm'",
    "build-umd": "tsc && webpack",
    "build-doc": "typedoc",
    "build-grammar-old": "NODE_ENV=development node -r esbuild-register ./scripts/grammar/bitmark/generate-bitmark.ts",
    "build-grammar-old-test": "NODE_ENV=development node -r esbuild-register ./scripts/grammar/bitmark/generate-bitmark.ts --test",
    "build-grammar-bit": "NODE_ENV=development node -r esbuild-register ./scripts/grammar/bitmark/generate-bitmark.ts --bit --test",
    "build-grammar-test": "NODE_ENV=development node -r esbuild-register ./scripts/grammar/test/generate-test.ts --test",
    "test": "jest --runInBand",
    "test-generator-standard": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-generator.test.ts --runInBand",
    "test-generator-books": "jest --config=./test/custom/generator/books/jest.generator.books.config.js --runInBand",
    "test-generator-expected": "jest --config=./test/custom/generator/expected/jest.generator.expected.config.js --runInBand",
    "test-parser-standard": "jest --config=jest.config.js --testMatch=\\<rootDir\\>/test/standard/bitmark-parser.test.ts --runInBand",
    "test-parser-books": "jest --config=./test/custom/parser/books/jest.parser.books.config.js --runInBand",
    "test-parser-expected": "jest --config=./test/custom/parser/expected/jest.parser.expected.config.js --runInBand",
    "publish": "yarn npm publish",
    "prepublish": "run lint && run clean && run build && run test",
    "clean": "shx rm -rf ./dist",
    "lint": "eslint --max-warnings 0 -- './**/*.@(tsx|ts|jsx|js|mjs|cjs)'",
    "ncu": "yarn dlx npm-check-updates -i",
    "tsc": "tsc",
    "jest": "jest"
  },
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=10",
    "npm": ">=6.8.0",
    "git": ">=2.11.0",
    "yarn": ">=3.5.0"
  },
  "keywords": [
    "bitmark"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitmark-standard/bmg.git"
  },
  "browserslist": {
    "modern": [
      "last 5 chrome versions",
      "last 5 firefox versions"
    ]
  },
  "jsdelivr": "dist/browser/bitmark-generator.min.js",
  "unpkg": "dist/browser/bitmark-generator.min.js",
  "contributors": [
    "Richard Sewell (https://github.com/zx-ncoderz)"
  ],
  "resolutions": {
    "antlr4ts": "0.5.0-alpha.4"
  },
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "esbuild": "^0.17.15",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-arca": "^0.16.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.2.2",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^14.0.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "peggy": "^3.0.2",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.3",
    "ts-pegjs": "^3.1.0",
    "typedoc": "^0.23.19",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@ncoderz/superenum": "^0.2.0",
    "@types/fs-extra": "^11.0.1",
    "bitmark-grammar": "1.0.69",
    "fs-extra": "^11.1.0"
  }
}
