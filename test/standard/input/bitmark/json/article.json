        "image": {
          "format": "png",
          "provider": "docs.bitmark.cloud",
          "src": "https://docs.bitmark.cloud/bit-books/beWirken/das_methodenbuch_fur_digitale_unterricht/web-resources/images/bewirken_k7_7.png",
          "width": 150,
