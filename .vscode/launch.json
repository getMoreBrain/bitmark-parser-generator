    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: Bitmark Parser - Standard Tests",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "args": ["--config=jest.config.js", "--testMatch", "<rootDir>/test/standard/bitmark-parser.test.ts", "-i"],
      // "preLaunchTask": "build",
      "console": "integratedTerminal"
      // "disableOptimisticBPs": true,
