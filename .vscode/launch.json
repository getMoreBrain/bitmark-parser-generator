  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Dev Bitmark Parser",
      "program": "${workspaceFolder}/test/custom/dev/bmgDevParser.ts",
      "runtimeArgs": ["-r", "esbuild-register"],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dev Bitmark Parser ANTLR",
      "program": "${workspaceFolder}/test/custom/dev/bmgDevParserAntlr.ts",
      "runtimeArgs": ["-r", "esbuild-register"],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dev Bitmark Generator",
      "program": "${workspaceFolder}/test/custom/dev/bmgDevGenerator.ts",
      "runtimeArgs": ["-r", "esbuild-register"],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dev Prettify",
      "program": "${workspaceFolder}/test/custom/dev/bmgDevPrettify.ts",
      "runtimeArgs": ["-r", "esbuild-register"],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: Current file",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "args": ["${fileBasenameNoExtension}"],
      // "preLaunchTask": "build",
      "console": "integratedTerminal"
      // "disableOptimisticBPs": true,
      // "internalConsoleOptions": "openOnSessionStart",
      // "outFiles": ["${workspaceRoot}/dist/**/*"],
      // "envFile": "${workspaceRoot}/.env",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: ALL Standard Tests",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "args": ["-i"],
      // "preLaunchTask": "build",
      "console": "integratedTerminal"
      // "disableOptimisticBPs": true,
      // "internalConsoleOptions": "openOnSessionStart",
      // "outFiles": ["${workspaceRoot}/dist/**/*"],
